{
  "defaultSeverity": "error",
  "extends": ["tslint:latest", "tslint-config-prettier"],
  "jsRules": {},
  "rules": {
    "no-console": [false, "log", "warn"],
    "no-var-requires": false,
    "max-classes-per-file": [true, 5],
    "no-implicit-dependencies": [true, "dev"],
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-leading-underscore"
    ]
  },
  "rulesDirectory": []
}
